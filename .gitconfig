# gitconfig

[user]
  email = simon.kaleschke@inaudito.de
  name = Simon Kaleschke
  signingkey = A9E66BBF378A8A26
[alias]
  co = checkout
  cf = !git checkout `git branch | fzf`
  br = branch
  cm = commit
  st = status
  pl = pull
  ps = push
  pu = !git push -u $(git remote) $(git branch --show-current)
  lg = log
  df = diff
  dfc = diff --cached
[color]
  ui = true
[pull]
  rebase = false
[init]
  defaultBranch = main
[commit]
  gpgsign = true
